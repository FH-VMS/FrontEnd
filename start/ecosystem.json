{
  "apps" : [{
    "name"        : "fycnWeb",
    "script"      : "app.js",
    "watch"       : true,
    "max_memory_restart": "800M",
    "exec_mode": "cluster_mode",
    "instances": 2,
    "log_file"   : "/dev/null",
    "error_file" : "/log/err.log",
    "out_file"   : "/log/out.log",
    "pid_file"   : "/log/pm2.pid",
    "merge_logs"      : false,
    "log_date_format" : "YYYY-MM-DD HH:mm:ss Z",
    "env": {
      "NODE_ENV": "development"
    },
    "env_production" : {
       "NODE_ENV": "production"
    },
    "port": 80
  }]
}